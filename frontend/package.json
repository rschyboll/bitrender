{
  "devDependencies": {
    "@babel/preset-env": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@parcel/compressor-brotli": "^2.6.2",
    "@parcel/compressor-gzip": "^2.6.2",
    "@parcel/reporter-bundle-analyzer": "^2.6.2",
    "@parcel/transformer-sass": "^2.6.2",
    "@parcel/validator-typescript": "^2.6.2",
    "@trivago/prettier-plugin-sort-imports": "^3.2.0",
    "@types/node": "^18.0.3",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.30.6",
    "@typescript-eslint/parser": "^5.30.6",
    "concurrently": "^7.2.2",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "parcel": "^2.6.2",
    "prettier": "^2.7.1",
    "process": "^0.11.10",
    "typescript": "^4.7.4",
    "yarn-upgrade-all": "^0.7.1"
  },
  "dependencies": {
    "@fontsource/lato": "^4.5.8",
    "ajv": "^8.11.0",
    "ajv-formats": "^2.1.1",
    "i18next": "^21.8.13",
    "i18next-browser-languagedetector": "^6.1.4",
    "immer": "^9.0.15",
    "include-media": "^1.4.10",
    "inversify": "^6.0.1",
    "inversify-react": "^1.0.2",
    "kea": "^3.0.2",
    "kea-localstorage": "^3.0.0",
    "kea-router": "^3.0.1",
    "kea-typegen": "^3.1.3",
    "ky": "^0.31.0",
    "primeflex": "^3.2.1",
    "primeicons": "^5.0.0",
    "primereact": "^8.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^11.18.0",
    "react-icons": "^4.4.0",
    "react-router-dom": "^6.3.0",
    "react-top-loading-bar": "^2.1.0",
    "react-transition-group": "^4.4.2",
    "reflect-metadata": "^0.1.13",
    "remixicon": "^2.5.0"
  },
  "alias": {
    "@/*": "./src/$1"
  },
  "scripts": {
    "parcel:watch": "parcel --host 0.0.0.0 ./src/index.html ./src/themes/*",
    "parcel:build": "parcel build --no-source-maps --config .parcelrc.build ./src/index.html ./src/themes/* ",
    "watch": "concurrently \"yarn parcel:watch\" \"yarn kea:watch\" -n APP,KEA -c blue,green",
    "kea:watch": "kea-typegen watch -r ./src -c ./tsconfig.json"
  },
  "prettier": {
    "printWidth": 80,
    "tabWidth": 2,
    "trailingComma": "all",
    "singleQuote": true,
    "semi": true,
    "importOrder": [
      "<THIRD_PARTY_MODULES>",
      "^@/(.*)$",
      "^[./]"
    ],
    "importOrderSeparation": true,
    "importOrderSortSpecifiers": true,
    "importOrderParserPlugins": [
      "typescript",
      "jsx",
      "decorators-legacy"
    ]
  }
}
