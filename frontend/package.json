{
  "devDependencies": {
    "@babel/preset-env": "^7.17.10",
    "@babel/preset-typescript": "^7.16.7",
    "@parcel/transformer-sass": "2.5.0",
    "@parcel/validator-typescript": "^2.5.0",
    "@trivago/prettier-plugin-sort-imports": "^3.2.0",
    "@types/node": "^17.0.33",
    "@types/react": "^18.0.9",
    "@types/react-dom": "^18.0.4",
    "concurrently": "^7.1.0",
    "eslint": "^8.15.0",
    "parcel": "^2.4.1",
    "prettier": "^2.6.2",
    "typescript": "^4.6.4"
  },
  "dependencies": {
    "@fontsource/nunito": "^4.5.9",
    "i18next": "^21.8.2",
    "i18next-browser-languagedetector": "^6.1.4",
    "include-media": "^1.4.10",
    "kea": "^3.0.0",
    "kea-typegen": "^3.0.0",
    "primeflex": "^3.2.0",
    "primeicons": "^5.0.0",
    "primereact": "^8.1.0",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-i18next": "^11.16.9",
    "react-router-dom": "^6.3.0"
  },
  "alias": {
    "@/*": "./src/$1"
  },
  "scripts": {
    "parcel:watch": "parcel --host 0.0.0.0 ./src/index.html ./src/themes/*",
    "watch": "concurrently \"yarn parcel:watch\" \"yarn kea:watch\" -n APP,KEA -c blue,green",
    "kea:watch": "kea-typegen watch -r ./src -c ./tsconfig.json"
  },
  "prettier": {
    "printWidth": 80,
    "tabWidth": 2,
    "trailingComma": "all",
    "singleQuote": true,
    "semi": true,
    "importOrder": [
      "<THIRD_PARTY_MODULES>",
      "^@/(.*)$",
      "^[./]"
    ],
    "importOrderSeparation": true,
    "importOrderSortSpecifiers": true
  }
}
