{
  "devDependencies": {
    "@parcel/validator-typescript": "^2.4.1",
    "@trivago/prettier-plugin-sort-imports": "^3.2.0",
    "@types/node": "^17.0.23",
    "@types/react": "^18.0.2",
    "@types/react-dom": "^18.0.0",
    "eslint": "^8.13.0",
    "parcel": "^2.4.1",
    "prettier": "^2.6.2",
    "typescript": "^4.6.3"
  },
  "dependencies": {
    "primeflex": "^3.1.3",
    "primeicons": "^5.0.0",
    "primereact": "^8.0.0-rc.1",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-router-dom": "^6.3.0"
  },
  "alias": {
    "@/*": "./src/$1"
  },
  "scripts": {
    "parcel:watch": "parcel --host 0.0.0.0 ./src/index.html"
  },
  "prettier": {
    "printWidth": 80,
    "tabWidth": 2,
    "trailingComma": "all",
    "singleQuote": true,
    "semi": true,
    "importOrder": [
      "<THIRD_PARTY_MODULES>",
      "^@/(.*)$",
      "^[./]"
    ],
    "importOrderSeparation": true,
    "importOrderSortSpecifiers": true
  }
}
